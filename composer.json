{
    "name": "oldsound/rabbitmq-bundle",
    "description": "Integrates php-amqplib with Symfony2 and RabbitMq",
    "keywords": ["symfony2", "rabbitmq", "message", "queue"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Alvaro Videla"
    }],
    "require": {
        "php"                      : ">=5.3.0",
        "symfony/framework-bundle" : ">=2.0,<2.3",
        "symfony/yaml": "~2.0",
        "videlalvaro/php-amqplib"  : "2.0.*"
    },
    "require-dev": {
        "symfony/console": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "OldSound\\RabbitMqBundle\\": ""
        }
    },
    "target-dir" : "OldSound/RabbitMqBundle"
}
